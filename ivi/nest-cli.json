{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api_gateway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/api_gateway/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/api_gateway",
  "projects": {
    "api_gateway": {
      "type": "application",
      "root": "apps/api_gateway",
      "entryFile": "main",
      "sourceRoot": "apps/api_gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api_gateway/tsconfig.app.json"
      }
    },
    "film": {
      "type": "application",
      "root": "apps/film",
      "entryFile": "main",
      "sourceRoot": "apps/film/src",
      "compilerOptions": {
        "tsConfigPath": "apps/film/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "person": {
      "type": "application",
      "root": "apps/person",
      "entryFile": "main",
      "sourceRoot": "apps/person/src",
      "compilerOptions": {
        "tsConfigPath": "apps/person/tsconfig.app.json"
      }
    },
    "genre": {
      "type": "application",
      "root": "apps/genre",
      "entryFile": "main",
      "sourceRoot": "apps/genre/src",
      "compilerOptions": {
        "tsConfigPath": "apps/genre/tsconfig.app.json"
      }
    },
    "country": {
      "type": "application",
      "root": "apps/country",
      "entryFile": "main",
      "sourceRoot": "apps/country/src",
      "compilerOptions": {
        "tsConfigPath": "apps/country/tsconfig.app.json"
      }
    },
    "award": {
      "type": "application",
      "root": "apps/award",
      "entryFile": "main",
      "sourceRoot": "apps/award/src",
      "compilerOptions": {
        "tsConfigPath": "apps/award/tsconfig.app.json"
      }
    }
  }
}